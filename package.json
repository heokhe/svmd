{
  "name": "svmd",
  "version": "0.1.0",
  "description": "Material design components for Svelte",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "scripts": {
    "build": "rollup -c",
    "build:dev": "rollup -cw",
    "serve": "sirv -s",
    "serve:dev": "sirv -sD",
    "start": "run-p serve:dev build:dev",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@hkh12/eslint-config": "^1.0.1",
    "eslint": "^6.1.0",
    "eslint-plugin-svelte3": "^2.7.3",
